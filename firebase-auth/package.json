{
  "private": true,
  "name": "@min-pack/firebase-auth",
  "version": "0.0.0",
  "main": "library/index.js",
  "scripts": {
    "// script ======================": "",
    "script-base": "babel-node ./script quiet",
    "script-pack": "babel-node ./script pack",
    "script-publish": "babel-node ./script pack publish",
    "script-publish-dev": "babel-node ./script pack publish-dev",
    "// build =======================": "",
    "build-library": "babel-node ./script/webpack production profile",
    "build-library-dev": "babel-node ./script/webpack development watch",
    "// =============================": "",
    "fast-install": "npm install --prefer-local --ignore-optional",
    "prepack": "echo \"Error: pack with script-*\" && exit 1"
  },
  "engines": {
    "node": ">=8.8",
    "npm": ">=5"
  },
  "devDependencies": {
    "dr-js": "^0.15.1",
    "dev-dep-tool": "0.3.1-dev.0",
    "dev-dep-web": "^0.3.0",
    "@firebase/polyfill": "^0.3.3",
    "@firebase/app": "^0.3.3",
    "@firebase/auth": "^0.5.3"
  }
}
